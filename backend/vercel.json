{
    "version": 2,
    "routes": [
      {
        "src": "/api/v1/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/uploads/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/uploads/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  